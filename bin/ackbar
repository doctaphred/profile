def play_audio(audio_data):
    # TODO: this is gross.
    with tempfile.NamedTemporaryFile(suffix='.mp3') as f:
        f.write(audio_data)
        os.system(f"afplay {f.name}")


def run_applescript(*words):
    # TODO: this is really gross.
    script = ' '.join(words)
    os.system(f"osascript -e '{script}'")


print(art, end='')


if not {'-q', '--quiet'} & set(sys.argv):
    run_applescript(
        r'tell',
        r'application',
        r'"System Events"',
        r'to',
        r'set',
        r'volume',
        r'output',
        r'volume',  # [sic]
        r'50',
    )
    play_audio(sound_bite)
